{
    "compilerOptions": {
        "noImplicitAny": true,
        "allowJs": true,
        "target": "es6",
        "jsx": "react",
        "lib": [
            "es2019",
            "dom",
            "dom.iterable"
        ],
        "esModuleInterop": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "rootDir": ".",
        "baseUrl": "."
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/index.tsx"
    ],
    "exclude": ["node_modules"]
}
