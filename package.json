{
  "name": "pcui",
  "version": "1.0.0",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "node-sass": "^4.14.1",
    "playcanvas": "^1.32.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "sass-loader": "^9.0.2"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --ext .js src",
    "storybook": "jsdoc -r -X src/components > ./.storybook/utils/jsdoc-ast.json && start-storybook -p 9009 -s public",
    "docs:local": "cd docs && jekyll build --config _config_local.yml && mkdir _site/storybook && cd .. && ENVIRONMENT=production build-storybook --no-dll -o ./docs/_site/storybook",
    "docs:build": "cd docs && jekyll build && mkdir _site/storybook && cd .. && ENVIRONMENT=production build-storybook --no-dll -o ./docs/_site/storybook",
    "pcui:publish": "npm run build && cp ./package.json dist && cp ./LICENSE dist && cp README.md dist && npm publish dist --dry-run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "@playcanvas/eslint-config",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@playcanvas/eslint-config": "^1.0.6",
    "@storybook/addon-actions": "^6.0.3",
    "@storybook/addon-backgrounds": "^6.0.3",
    "@storybook/addon-controls": "^6.0.0-beta.15",
    "@storybook/addon-docs": "^6.0.3",
    "@storybook/addon-links": "^6.0.3",
    "@storybook/addons": "^6.0.3",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.3",
    "babel-loader": "^8.1.0",
    "eslint": "^7.10.0",
    "jsdoc": "^3.6.5",
    "json-loader": "^0.5.7",
    "prop-types": "^15.7.2",
    "react-app-rewire-alias": "^0.1.6",
    "react-app-rewired": "^2.1.6",
    "react-is": "^16.13.1",
    "webpack-cli": "^3.3.12"
  }
}
